{
    "name": "binary-bot",
    "version": "1.0.0",
    "description": "Visual automation for binary.com",
    "bin": {
        "bot": "lib/index.js"
    },
    "engine": {
        "node": ">=18.16.0"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>/.jest"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/",
            "tools.js"
        ]
    },
    "scripts": {
        "test": "eslint src/",
        "release": "d() { test -z $1 && echo 'Please specify branch.' && exit 1; (git show-branch $1) || exit 1;  git stash; git checkout $1; npm i; rm -rf branch/$1; mkdir -p branch/$1 ; gulp build-min --test --branch $1; cp -r www/ branch/$1; cp www/index.html www/404.html; gulp release-branch --branch $1;}; d",
        "release-production": "d() { git stash; git checkout master; npm i;gulp build-min;gulp release-master;}; d",
        "build:1": "webpack --config webpack.config.cli.js",
        "build-min": "webpack --config webpack.config.cli.js",
        "prepare": "husky install",
        "prettify": "npx prettier --write **/{src}/**/*.{js,jsx,ts,tsx,md,html,mustache,css,scss,}",
        "fetch-translations": "node customPlugins/fetch-blockly-translations.js",
        "start": "npm run fetch-translations && webpack serve --open",
        "build": "webpack --config webpack.config.cli.js --mode production",
        "cleanup": "rm -rf node_modules www temp translations",
        "cleanup-cache": "rm -rf www temp translations"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "author": "Binary.com",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "^7.16.8",
